{
   "background": {
      "scripts": [ "js/mootools-core-1.4.5-full-nocompat-yc.js", "js/nzbget.js" ]
   },
   "description": "Interact with and send NZB-files to NZBGet",
   "icons": {
      "16": "icon/icon16x16.png"
   },
   "manifest_version": 2,
   "web_accessible_resources": [
    "icon/logo.png"
   ],
   "name": "nzbget-chrome",
   "options_page": "options.html",
   "permissions": [ "*://*/", "contextMenus", "tabs", "notifications", "storage" ],
   "version": "0.1",
   "browser_action": {
      "default_icon": "icon/icon16x16.png",
      "default_popup": "popup.html"
   }
}
